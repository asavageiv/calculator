{
    "env": {
        "browser": true,
        "es6": true,
        "node": true,
        "jasmine": true
    },
    "extends": ["eslint:recommended", "google", "plugin:jasmine/recommended"],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": ["jasmine"],
    "rules": {
      "jasmine/new-line-between-declarations": "off",
      "jasmine/no-spec-dupes": "off",
      "require-jsdoc": "off"
    }
}
